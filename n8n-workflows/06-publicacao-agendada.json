{
  "name": "BASE Agency - PublicaÃ§Ã£o Agendada",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "minutes",
              "minutesInterval": 15
            }
          ]
        }
      },
      "id": "cron-publish",
      "name": "Cron 15 min",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.2,
      "position": [250, 300]
    },
    {
      "parameters": {
        "method": "GET",
        "url": "https://agenciabase.onrender.com/api/demands/scheduled-now",
        "options": {}
      },
      "id": "get-scheduled",
      "name": "Buscar Agendadas",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [450, 300]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "leftValue": "={{ $json.demands.length }}",
              "rightValue": 0,
              "operator": {
                "type": "number",
                "operation": "gt"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "check-scheduled",
      "name": "Tem Agendadas?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [650, 300]
    },
    {
      "parameters": {
        "fieldToSplitOut": "demands",
        "options": {}
      },
      "id": "split-scheduled",
      "name": "Para Cada Demanda",
      "type": "n8n-nodes-base.splitOut",
      "typeVersion": 1,
      "position": [850, 200]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "https://api.late.so/v1/posts",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Authorization",
              "value": "Bearer {{$node['Config'].json.late_api_key}}"
            }
          ]
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "account_id",
              "value": "={{$json.account_id}}"
            },
            {
              "name": "content",
              "value": "={{$json.content}}"
            },
            {
              "name": "publish_now",
              "value": "true"
            }
          ]
        },
        "options": {}
      },
      "id": "publish-late",
      "name": "Publicar via Late",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [1050, 200]
    },
    {
      "parameters": {
        "method": "PATCH",
        "url": "https://agenciabase.onrender.com/api/demands/{{$json.demand_id}}/status",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "status",
              "value": "published"
            }
          ]
        },
        "options": {}
      },
      "id": "update-status",
      "name": "Atualizar Status",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [1250, 200]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "https://api.z-api.io/instances/{{$node['Config'].json.zapi_instance}}/token/{{$node['Config'].json.zapi_token}}/send-text",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "phone",
              "value": "={{$json.client_phone}}"
            },
            {
              "name": "message",
              "value": "ðŸš€ Seu conteÃºdo foi publicado!\n\n*{{$json.title}}*\n\nAcompanhe os resultados!"
            }
          ]
        },
        "options": {}
      },
      "id": "notify-client",
      "name": "Notificar Cliente",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [1450, 200]
    }
  ],
  "connections": {
    "Cron 15 min": {
      "main": [
        [
          {
            "node": "Buscar Agendadas",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Buscar Agendadas": {
      "main": [
        [
          {
            "node": "Tem Agendadas?",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Tem Agendadas?": {
      "main": [
        [
          {
            "node": "Para Cada Demanda",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Para Cada Demanda": {
      "main": [
        [
          {
            "node": "Publicar via Late",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Publicar via Late": {
      "main": [
        [
          {
            "node": "Atualizar Status",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Atualizar Status": {
      "main": [
        [
          {
            "node": "Notificar Cliente",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {
    "executionOrder": "v1"
  }
}
